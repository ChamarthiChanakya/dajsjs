a=5
if a>5:
for i in range(2,a):
	if a%i==0:
if a%i=0:print("not a prime")
break
else:
print("prime")
break
else:
print("prime")
else:
print("not a prime")



def squaressum(n):
return(n*(n+1)/2)+(2*(n+1)/3)
n=8
print(squaressum(n) ) 


